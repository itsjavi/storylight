{
  "name": "@storylite/vite-plugin",
  "version": "0.12.1",
  "description": "A lightweight alternative to StoryBook. This package contains the Vite plugin.",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./virtual": {
      "types": "./dist/virtual-modules.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "virtual": [
        "dist/virtual-modules.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsup --watch",
    "lint": "eslint --max-warnings=10 .",
    "lint-fix": "eslint --fix .",
    "prepublishOnly": "pnpm run build",
    "publint-check": "pnpm run build && pnpm publint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "vite": "^4.4.9"
  },
  "devDependencies": {
    "@r1stack/coding-style": "^0.4.5",
    "@types/node": "^20.6.4",
    "publint": "^0.2.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
