{
  "name": "@storylite/storylite",
  "version": "0.6.0",
  "description": "A lightweight alternative to StoryBook, built on top of Vite⚡️. This package contains the React app.",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./styles.css": "./dist/index.css"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsup --watch",
    "lint": "eslint --max-warnings=10 .",
    "lint-fix": "eslint --fix .",
    "prepublishOnly": "pnpm run build",
    "publint-check": "pnpm publint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@r1stack/core": "^0.4.3",
    "lucide-react": "^0.271.0"
  },
  "devDependencies": {
    "@r1stack/coding-style": "^0.4.3",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "publint": "^0.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "peerDependencies": {
    "@types/node": "^20.4.9",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "typescript": "^5.1.6"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    },
    "react-router-dom": {
      "optional": false
    },
    "typescript": {
      "optional": false
    }
  }
}
