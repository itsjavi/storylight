{
  "name": "@storylite/storylite",
  "version": "0.14.0",
  "description": "A lightweight alternative to StoryBook, built on top of Vite⚡️. This package contains the React app.",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./styles.css": "./dist/index.css"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsup --watch",
    "lint": "biome lint .",
    "lint-fix": "biome lint --apply .",
    "prepublishOnly": "pnpm run build",
    "publint-check": "pnpm publint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.72",
    "@types/react-dom": "^18.2.22",
    "publint": "^0.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "@types/node": "^20.4.9",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    },
    "typescript": {
      "optional": false
    }
  }
}
